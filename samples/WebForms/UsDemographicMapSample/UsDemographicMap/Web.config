<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="CategoryFilePath" value="~/App_Data/CategoryList.xml" />
    <add key="UsShapefilePath" value="~/App_Data/usStatesCensus2010.shp" />
  </appSettings>
  <connectionStrings />
  <system.web>
    <httpHandlers>
      <add path="*_GeoResource.axd" verb="*" type="ThinkGeo.MapSuite.WebForms.GeoResourceFactory" />
    </httpHandlers>
    <compilation debug="true" />
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <handlers>
      <add name="GeoResource" preCondition="integratedMode" path="*_GeoResource.axd" verb="*" type="ThinkGeo.MapSuite.WebForms.GeoResourceFactory" />
    </handlers>
  </system.webServer>
</configuration>